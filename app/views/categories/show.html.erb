<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>

<p>
  <strong>Desc:</strong>
  <%= @category.desc %>
</p>

<h3> Listing under this category </h3>
<% Listing.where(category_id: @category.id).each do |listing| %>
<li> <%= link_to listing.title, listing_path(id:listing, isApplication: "false")%> </li>
<% end %>

<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
